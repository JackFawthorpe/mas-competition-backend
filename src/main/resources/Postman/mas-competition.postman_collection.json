	"item": [
		{
			"name": "authentication",
			"item": [
				{
					"name": "Login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"const jessionID = pm.response.cookies.find(cookie => cookie.name === 'JSESSIONID');\r",
									"\r",
									"if (jessionID) {\r",
									"    pm.collectionVariables.set(\"JSESSIONID\", jessionID.value);\r",
									"}"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "formdata",
							"formdata": [
								{
									"key": "email",
									"value": "User-1@email.com",
									"type": "text"
								},
								{
									"key": "password",
									"value": "ValidPassword1!",
									"type": "text"
								}
							]
						},
						"url": {
							"raw": "{{baseURL}}/login",
							"host": [
								"{{baseURL}}"
							],
							"path": [
								"login"
							]
						}
					},
					"response": []
				},
				{
					"name": "Logout",
					"request": {
						"method": "POST",
						"header": [],
						"url": {
							"raw": "{{baseURL}}/logout",
							"host": [
								"{{baseURL}}"
							],
							"path": [
								"logout"
							]
						}
					},
					"response": []
				}
			]
		},
		{
			"name": "admin",
			"item": [
				{
					"name": "AddUsers",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 201) {\r",
									"    let currentCount = Number(pm.collectionVariables.get(\"createdUserCount\"));\r",
									"    currentCount += 1;\r",
									"    pm.collectionVariables.set(\"createdUserCount\", currentCount.toString());\r",
									"}"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "[\r\n    { \"email\": \"User-{{createdUserCount}}@email.com\", \"password\": \"ValidPassword1!\"}\r\n]",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{baseURL}}/admin/users",
							"host": [
								"{{baseURL}}"
							],
							"path": [
								"admin",
								"users"
							]
						}
					},
					"response": []
				}
			]
		}
	],
	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"packages": {},
				"exec": [
					"\r",
					"const jsessionId = pm.environment.get('JSESSIONID');\r",
					"\r",
					"if (jsessionId) {\r",
					"    pm.request.headers.add({\r",
					"        key: 'Cookie',\r",
					"        value: `JSESSIONID=${jsessionId}`\r",
					"    });\r",
					"}"
				]
			}
		},
		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"packages": {},
				"exec": [
					""
				]
			}
		}
	],
	"variable": [
		{
			"key": "email",
			"value": "admin"
		},
		{
			"key": "baseURL",
			"value": "http://localhost:4000/api/v1"
		},
		{
			"key": "password",
			"value": "admin"
		},
		{
